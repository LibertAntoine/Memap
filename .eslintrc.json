{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "rules": {
		"no-unused-vars": "warn",
		"no-mixed-spaces-and-tabs": "off",
    "no-console": "off",
		"no-debugger":"off"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
